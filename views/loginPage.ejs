<nav class="navbar  navbar-dark  navbar-expand-sm fixed-top" id="navani">

            <div class="navbar-header" style="color:white" id="nav_head">
                    <img src="/Images/projectlogo.svg" alt="logo" width="40px" height="40px" style="padding-right:0px;padding-bottom: 4px">
                    <div class="navbar-brand">WorkSpace</div>
                    
            </div>

            
    
            <button class="navbar-toggler" data-toggle="collapse" data-target="#cnb" id="hamBtn">
              <img src="/CSS/menu.svg" alt="" width="20px" >
                    
            </button>
    
                     
</nav>

<div class="mainDiv container-fluid">
        <div class="row">
                <div class="col-5">

                </div>
                <div class="col-7">
                        <div class="whiteBox p-5">
                       
                                <h3 style="font-weight: bold;" class="mb-4">Administrator login</h3>
                                <% if(error && error.length > 0){%>
                            
                                  <p style="color: red;" class="mb-2"><b><%= error %></b></p>
                                
                                <% } %>
                                <% if(success && success.length > 0){%>
                            
                                  <p style="color: blue;" class="mb-2"><b><%= success %></b></p>
                                
                                <% } %>
                              
                                <form action="/workspace/admin/login" method="POST">
                                        <input type="text" name="username" id="" required autocomplete="off" class="inpFld" placeholder="Enter your username">
                                        <input type="password" name="password" id="" required autocomplete="off" class="inpFld mt-4" placeholder="Enter your password">
                                        
                                        <button type="submit" class="logBtn mt-4">LOGIN</button>
                                        <hr class="mt-4">
                                        <p style="color: gray;">Only registered WorkSpace administrators can use this site for platform maintenance.</p>
                                </form>
                        </div>
                </div>
        </div>
</div>




